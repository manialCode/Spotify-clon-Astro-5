{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/components": [
        "src/components/*"
      ],
      "@/components/react": [
        "src/components/react*"
      ],
      "@/hooks": [
        "src/hooks/*"
      ],
      "@/icons": [
        "src/icons/*"
      ],
      "@/lib": [
        "src/lib/*"
      ],
      "@/services": [
        "src/services/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}